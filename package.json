{
  "private": true,
  "scripts": {
    "build": "trash story/dist && parcel build src/+dist/*",
    "build:prod": "cross-env NODE_ENV=production yarn run build --no-source-maps",
    "build:story": "yarn run clean && build-storybook -c story -o story/dist",
    "build:watch": "yarn run clean && parcel watch src/+dist/*",
    "clean": "trash .parcel-cache dist",
    "coverage": "yarn run test --coverage",
    "coverage:watch": "yarn run test:coverage --watch",
    "lint": "tsc && yarn run lint:file .",
    "lint:file": "eslint",
    "lint:fix": "yarn run lint:fix:file .",
    "lint:fix:file": "yarn run lint:file --fix",
    "server": "parcel serve src/+dist/*",
    "server:story": "start-storybook -c story",
    "start": "yarn run build:prod",
    "test": "jest",
    "test:update": "yarn run test --update-snapshot",
    "test:watch": "yarn run test --watch"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recoil": "^0.0.10",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@jupl/ts": "^0.7.0",
    "@storybook/react": "^5.2.8",
    "@types/jest": "^26.0.5",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/recoil": "^0.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "jest": "^26.1.0",
    "jest-styled-components": "^7.0.2",
    "parcel": "^2.0.0-nightly.341",
    "postcss-modules": "^3.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.10",
    "trash-cli": "^3.0.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.7"
  }
}
