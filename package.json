{
  "private": true,
  "scripts": {
    "build": "yarn run clean && parcel build src/+dist/*",
    "build:prod": "cross-env NODE_ENV=production yarn run build --no-source-maps",
    "build:story": "trash story/dist && build-storybook -c story -o story/dist",
    "build:watch": "yarn run clean && parcel watch src/+dist/*",
    "clean": "trash .parcel-cache dist",
    "coverage": "yarn run test --coverage",
    "coverage:watch": "yarn run coverage --watch",
    "lint": "yarn run lint:type && yarn run lint:file .",
    "lint:file": "eslint",
    "lint:fix": "yarn run lint:fix:file .",
    "lint:fix:file": "yarn run lint:file --fix",
    "lint:type": "tsc",
    "parcel": "parcel",
    "server": "parcel serve src/+dist/*",
    "server:story": "start-storybook --ci -c story -p 9001",
    "start": "yarn run lint && yarn run test && yarn run build:prod",
    "test": "jest",
    "test:update": "yarn run test --update-snapshot",
    "test:watch": "yarn run test --watch"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "recoil": "^0.0.13",
    "styled-components": "^5.2.0",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@jest/globals": "^26.4.2",
    "@jupl/ts": "^0.10.0",
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-storyshots": "^6.0.21",
    "@storybook/addon-viewport": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.22",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.5.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "^26.4.2",
    "jest-styled-components": "^7.0.3",
    "parcel": "^2.0.0-beta.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.11",
    "trash-cli": "^3.1.0",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.3"
  }
}
