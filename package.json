{
  "private": true,
  "scripts": {
    "build": "yarn run clean && parcel build src/+dist/*",
    "build:prod": "cross-env NODE_ENV=production yarn run build --no-source-maps",
    "build:watch": "yarn run clean && parcel watch src/+dist/*",
    "clean": "trash .parcel-cache dist",
    "coverage": "yarn run test --coverage",
    "coverage:watch": "yarn run coverage --watch",
    "lint": "yarn run lint:type && yarn run lint:file .",
    "lint:file": "eslint",
    "lint:fix": "yarn run lint:fix:file .",
    "lint:fix:file": "yarn run lint:file --fix",
    "lint:type": "tsc",
    "server": "parcel serve src/+dist/*",
    "start": "yarn run lint && yarn run test && yarn run build:prod",
    "test": "jest",
    "test:update": "yarn run test --update-snapshot",
    "test:watch": "yarn run test --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@jest/globals": "^26.4.2",
    "@jupl/ts": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.5.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "^26.4.2",
    "parcel": "^2.0.0-beta.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "sass": "^1.26.11",
    "trash-cli": "^3.1.0",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
